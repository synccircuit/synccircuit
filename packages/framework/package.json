{
  "name": "@synccircuit/framework",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build:clean": "del-cli dist/*",
    "build:check": "tsc --noEmit",
    "build:compile": "tsc --build && tsc-alias -p tsconfig.json",
    "build": "yarn build:clean && yarn build:check && yarn build:compile",
    "lint": "prettier --check . && oxlint",
    "format": "prettier --write ."
  },
  "type": "module",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "author": "WoozyStudio <woozystudiocontact@gmail.com>",
  "keywords": [],
  "private": true,
  "license": "AGPL-3.0",
  "dependencies": {
    "@discordjs/core": "2.0.1",
    "@discordjs/rest": "2.4.3",
    "colors": "1.4.0",
    "discord-api-types": "0.37.119",
    "discord.js": "14.18.0"
  },
  "devDependencies": {
    "@types/node": "22.13.10",
    "cpy-cli": "5.0.0",
    "del-cli": "6.0.0",
    "prettier": "3.5.3",
    "tsc-alias": "^1.8.11",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
